alias ad='git add'
alias ar='git add .'
alias ca='git diff --cached'
alias cam='git commit --amend'
alias ci='git commit'
alias cia='git commit -a'
alias cii='git commit -i'
alias clean='git clean -ndX'
alias co='git checkout'
alias cob='git checkout -b'
alias di='git diff'
alias dic='git diff --cached'
